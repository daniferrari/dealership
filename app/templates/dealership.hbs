<h1>Dealership List</h1>

{{#link-to 'dealership.new' class="btn btn-primary"}}Add new dealership{{/link-to}}

<br><br>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |dealership|}}
    <tr>
      <td>{{dealership.name}}</td>
      <td>{{dealership.address}}</td>
      <!-- <td>{{#link-to 'dealership.listcars' }}<button class="btn btn-info btn-xs">Details</button>{{/link-to}}</td> -->
    </tr>
  {{/each}}
  </tbody>
</table>

{{outlet}}
